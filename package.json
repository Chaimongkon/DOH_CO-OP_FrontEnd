    "antd": "^5.20.1",
    "antd-style": "^3.6.2",
    "busboy": "^1.6.0",
    "canvas-confetti": "^1.9.3",
    "date-fns": "^4.0.0",
    "formidable": "^3.5.1",
    "ioredis": "^5.4.1",
    "js-confetti": "^0.12.0",
    "js-cookie": "^3.0.5",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.4.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.0",
