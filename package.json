{
  "name": "webcoop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "cross-env NEXT_DISABLE_WARNINGS=true next dev",
    "build": "next build",
    "build:win": "node scripts/fix-permissions.js && npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "quality-check": "npm run type-check && npm run lint",
    "build-check": "npm run quality-check && npm run build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "fix-permissions": "node scripts/fix-permissions.js",
    "clean": "node scripts/fix-permissions.js",
    "dev:win": "node scripts/fix-permissions.js && npm run dev",
    "check-api-standards": "node scripts/check-api-standards.js",
    "security-audit": "node scripts/security-audit.js",
    "security-test": "node scripts/security-test.js",
    "api-health": "npm run check-api-standards && npm run security-audit",
    "security-full": "npm run security-test && npm run security-audit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPatterns=api",
    "test:components": "jest --testPathPatterns=components",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
